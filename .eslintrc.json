{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "react/prop-types": 0,
      "quotes": [2, "double"],
      "semi": [2, "always"],
      "no-console": 2
    },
    "settings": {
      "react": {
        "pragma": "React",
        "version": "detect"
      }
    }
  }